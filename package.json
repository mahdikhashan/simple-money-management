    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-modal": "^3.15.1",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.2",
    "react-refresh": "^0.11.0",
