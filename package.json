{
  "name": "simple-money-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-dev": "NODE_ENV=development webpack serve --env development --mode development --open --config config/webpack.dev.config.js",
    "start-dev:generate-sample-data": "npm run start-dev && npm run generate-sample-data",
    "generate-sample-data": "node scripts/generate-sample-data.mjs",
    "webpack-help": "npx webpack --help=verbose | grep NODE_ENV",
    "build": "NODE_ENV=production webpack --progress --config config/webpack.prod.config.js",
    "test": "jest --config ./jest.config.js",
    "test:watch": "jest --watchAll --config ./jest.config.js",
    "test:e2e": "npx playwright test",
    "test:e2e:watch": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "test:visual-regression": "loki test --force --port 6006",
    "test:visual-regression:update": "loki update --force --port 6006",
    "test:ci:visual-regression": "pnpm run build-storybook && loki --requireReference --reactUri file:./storybook-static",
    "prettify": "yarn prettier . --write",
    "check-preittifier": "npx prettier . --check",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ci-build-storybook": "pnpm install --frozen-lockfile"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
